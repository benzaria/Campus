{
    "YesNo": [
        "<toast launch='${appProtocol}://?action=click' activationType='protocol'>",
        "  <visual>",
        "    <binding template='ToastGeneric'>",
        "      <text>Would you like to proceed?</text>",
        "      <text>Click Yes or No to continue.</text>",
        "    </binding>",
        "  </visual>",
        "  <actions>",
        "    <action content='Yes' arguments='${appProtocol}://?action=yes' activationType='protocol' />",
        "    <action content='No' arguments='${appProtocol}://?action=no' activationType='protocol' />",
        "  </actions>",
        "</toast>"
    ],
    "AcceptDismiss": [
        "<toast launch='${appProtocol}://?action=click' activationType='protocol'>",
        "  <visual>",
        "    <binding template='ToastGeneric'>",
        "      <text>Return books to the library.</text>",
        "    </binding>",
        "  </visual>",
        "  <actions>",
        "    <action content='Accept' arguments='${appProtocol}://?action=accept' activationType='protocol' imageUri='${dirname}/icon.png' />",
        "    <action content='Dismiss' arguments='${appProtocol}://?action=dismiss' activationType='protocol' />",
        "  </actions>",
        "</toast>"
    ],
    "AcceptSnoozeDismiss": [
        "<toast launch='${appProtocol}://?action=click' activationType='protocol'>",
        "  <visual>",
        "    <binding template='ToastGeneric'>",
        "      <text>Return books to the library.</text>",
        "    </binding>",
        "  </visual>",
        "  <actions>",
        "    <action content='Accept' arguments='${appProtocol}://?action=accept' activationType='protocol' imageUri='${dirname}/icon.png'/>",
        "    <action content='Snooze' arguments='${appProtocol}://?action=snooze' activationType='protocol' />",
        "    <action content='Dismiss' arguments='${appProtocol}://?action=dismiss' activationType='protocol' />",
        "  </actions>",
        "</toast>"
    ]
}